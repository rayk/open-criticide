{
  "name": "criticide",
  "productName": "Criticide",
  "version": "0.0.2",
  "description": "Probabilistic bayesian modelling for critical decisions in complex systems.",
  "main": "./src/index.js",
  "scripts": {
    "lint": "echo \"No linting configured\"",
    "make": "electron-forge make",
    "packMac": "electron-packager ./src --platform=darwin --arch=x64 --overwrite --icon=assets/icons/critLogo.icns --prune=true --out=out",
    "start": "electron-forge start",
    "test": "node --trace-warnings testSuite.js",
    "testCi": "jest && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "devDoc": "node ./node_modules/.bin/esdoc",
    "storybook": "start-storybook -p 9001 -c storybook",
    "package": "electron-forge package"
  },
  "keywords": [],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/rayk/criticide"
    }
  ],
  "engines": {
    "node": ">= 8.1.2"
  },
  "private": true,
  "author": "rayk",
  "contributors": [
    {
      "name": "Ji Young Lee",
      "email": "jylee1103@gmail.com",
      "url": "https://github.com/jiyounglee"
    },
    {
      "name": "Ray King",
      "email": "taipan.king@gmail.com",
      "url": "https://github.com/rayk"
    }
  ],
  "license": "MPL-2.0",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testMatch": [
      "<rootDir>/test/unit/**/*.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverageFrom": [
      "**/src/starup/*.js",
      "**/src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/out/**",
      "!**/coverage/**",
      "!**/test/**",
      "!**/index.js",
      "!**/envProps.js"
    ]
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "overwrite": true,
        "ignore": ".+.idea+.*",
        "packageManager": "yarn",
        "icon": "assets/icons/critLogo.icns"
      },
      "electronWinstallerConfig": {
        "name": "criticide"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "criticide"
      }
    }
  },
  "dependencies": {
    "assert-plus": "^1.0.0",
    "electron-compile": "^6.4.1",
    "electron-devtools-installer": "^2.2.0",
    "electron-localshortcut": "^2.0.2",
    "electron-log": "^2.2.7",
    "electron-window-state": "^4.1.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "ramda-adjunct": "^1.12.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "rollbar": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.0",
    "@storybook/react": "^3.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-spawn": "^5.1.0",
    "devtron": "^1.4.0",
    "electron-prebuilt-compile": "1.7.6",
    "enzyme": "^2.9.1",
    "esdoc": "^1.0.1",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "prettier": "^1.5.2",
    "react-test-renderer": "^15.6.1",
    "spectron": "^3.7.2"
  },
  "babel": {
    "plugins": [
      "add-module-exports",
      "transform-async-to-generator",
      "transform-es2015-classes",
      "transform-inline-imports-commonjs",
      "transform-object-rest-spread"
    ]
  },
  "esdoc": {
    "source": "./src",
    "destination": "./docs",
    "includes": [
      ".*\\.js",
      ".*\\.jsx"
    ],
    "excludes": [
      ".*\\.json"
    ],
    "index": "./resources/guide/index.md",
    "plugins": [
      {
        "name": "esdoc-jsx-plugin",
        "option": {
          "enable": true
        }
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": {
          "all": true
        }
      },
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "typeInference": {
            "enable": true
          },
          "brand": {
            "logo": "./assets/icons/png/critLogo.png_48x48.png",
            "title": "Criticide Developer Documentation",
            "description": "Auto generated code documentation"
          },
          "test": {
            "source": "./test/unit/",
            "interfaces": [
              "describe",
              "it"
            ]
          }
        }
      }
    ]
  }
}
